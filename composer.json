{
	"name": "cloudrly/wordpress",
	"minimum-stability": "dev",
	"description": "WordPress is open source software you can use to create a beautiful website, blog, or app.",
	"keywords": [
		"wordpress",
		"blog",
		"cms"
	],
	"type": "package",
	"homepage": "https://wordpress.org/",
	"license": "GPL-3.0+",
	"authors": [
		{
		"name": "WordPress Community",
		"homepage": "https://wordpress.org/about/"
		}
	],
	"support": {
		"issues": "https://core.trac.wordpress.org/",
		"forum": "https://wordpress.org/support/",
		"docs": "https://developer.wordpress.org/",
		"irc": "irc://irc.freenode.net/wordpress",
		"source": "https://core.trac.wordpress.org/browser"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "cloudrly/wordpress-core",
				"type": "webroot",
				"version": "6.1.1",
				"dist": {
					"url": "https://wordpress.org/wordpress-6.1.1.tar.gz",
					"type": "tar"
				}
			}
		}
	],
	"extra" : {
		"webroot-package": "cloudrly/wordpress-core"
	},
	"require" : {
		"cloudrly/wordpress-core" : "^6",
		"cloudrly/webroot-installer": "^1",
		"php": "^8"
	},
	"provide": {
		"wordpress/wordpress-core": "*"
	},
	"config": {
		"allow-plugins": {
			"cloudrly/webroot-installer": true
		}
	}
}
